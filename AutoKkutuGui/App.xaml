<Application
    x:Class="AutoKkutuGui.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:AutoKkutuGui"
    ShutdownMode="OnMainWindowClose"
    StartupUri="MainWindow.xaml">
    <Application.Resources>
        <FontFamily x:Key="NanumGothic">pack://application:,,,/Fonts/#NanumGothic</FontFamily>
        <FontFamily x:Key="NanumBarunGothic">pack://application:,,,/Fonts/#NanumBarunGothic</FontFamily>
    </Application.Resources>
</Application>
