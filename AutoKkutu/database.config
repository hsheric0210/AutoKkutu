<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<configSections>
		<section name="dbtype" type="AutoKkutu.ConfigFile.DatabaseTypeSection, AutoKkutu" />
		<section name="sqlite" type="AutoKkutu.ConfigFile.SQLiteSection, AutoKkutu" />
		<section name="mysql" type="AutoKkutu.ConfigFile.MySQLSection, AutoKkutu" />
		<section name="postgresql" type="AutoKkutu.ConfigFile.PostgreSQLSection, AutoKkutu" />
	</configSections>
	<!-- Available values: Local, MySQL, PostgreSQL, MariaDB -->
	<!-- There is no actual difference between MySQL and MariaDB. Only logo changes. -->
	<dbtype type="local" />
	<sqlite file="path.sqlite" />
	<mysql connectionString="Server=;Port=;Uid=;Pwd=;Database=;Charset=utf8" />
	<postgresql connectionString="Host=;Port=;Username=;Password=;Database=" />
</configuration>
